{
  "name": "express-typescript-boilerplate",
  "version": "0.0.0",
  "description": "express-typescript-boilerplate",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "concurrently npm:watch:*",
    "start": "node dist/server.js",
    "check:lint": "eslint --fix",
    "check:prettier": "prettier --config .prettierrc.json --write src/**/*.ts",
    "check:all": "npm run check:lint && npm run check:prettier",
    "watch:js": "nodemon -q dist/server.js",
    "watch:tsc": "npx tsc --watch"
  },
  "author": "Jonathan Hern√°ndez <jhzapata.seven@gmail.com>",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": 500
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.16",
    "prettier": "2.7.1",
    "typescript": "^4.7.4"
  }
}
